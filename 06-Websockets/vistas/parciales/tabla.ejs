<table style="width:50%">
  <tr>
    <th id="id">ID</th>
    <th id="titleHeader">Nombre</th>
    <th id="priceHeader">Precio</th>
    <th id="thumnailHeader">link</th>
  </tr>
  <% contenedor.forEach(anItem=> { %>
    <tr>
      <td>
        <%= anItem.id %>
      </td>
      <td>
        <%= anItem.title %>
      </td>
      <td>
        <%= anItem.price %>
      </td>
      <td>
        <%= anItem.thumnail %>
      </td>
    </tr>
    <% }) %>
</table>
